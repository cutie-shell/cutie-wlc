<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.cutie_shell.SettingsDaemon.Backlight">
    <method name="GetMaxBrightness">
      <arg name="brightness" type="u" direction="out"/>
    </method>
    <method name="GetBrightness">
      <arg name="brightness" type="u" direction="out"/>
    </method>
    <method name="SetBrightness">
      <arg name="brightness" type="u" direction="in"/>
    </method>
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.Atmosphere">
    <method name="GetPath">
      <arg name="path" type="s" direction="out"/>
    </method>
    <method name="GetVariant">
      <arg name="variant" type="s" direction="out"/>
    </method>
    <method name="SetPath">
      <arg name="path" type="s" direction="in"/>
    </method>
    <method name="SetVariant">
      <arg name="variant" type="s" direction="in"/>
    </method>
    <signal name="PathChanged">
    </signal>
    <signal name="VariantChanged">
    </signal>
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.Modems">
    <method name="ModemCount">
      <arg name="count" type="u" direction="out"/>
    </method>
    <signal name="ModemAdded">
      <arg name="path" type="o" direction="out"/>
    </signal>
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.Modem">
    <method name="GetOnline">
      <arg name="online" type="b" direction="out"/>
    </method>
    <method name="OnlineModem">
      <arg name="online" type="b" direction="in"/>
    </method>
    <signal name="OnlineChanged">
      <arg name="online" type="b" direction="out"/>
    </signal>
    <method name="GetPower">
      <arg name="power" type="b" direction="out"/>
    </method>
    <method name="PowerModem">
      <arg name="power" type="b" direction="in"/>
    </method>
    <signal name="PowerChanged">
      <arg name="power" type="b" direction="out"/>
    </signal>
    <method name="GetNetName">
      <arg name="name" type="s" direction="out"/>
    </method>
    <signal name="NetNameChanged">
      <arg name="name" type="s" direction="out"/>
    </signal>
    <method name="GetNetStrength">
      <arg name="strength" type="y" direction="out"/>
    </method>
    <signal name="NetStrengthChanged">
      <arg name="strength" type="y" direction="out"/>
    </signal>
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.Networking">
    <property name="Backends" type="ao" access="read"/>
    <signal name="BackendsChanged">
      <arg type="ao" name="Backends" direction="out"/>
    </signal>  
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.NetworkBackend">
    <property name="Devices" type="ao" access="read"/>
    <signal name="DevicesChanged">
      <arg type="ao" name="Devices" direction="out"/>
    </signal>  
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.NetworkDevice">
    <property name="Services" type="ao" access="read"/>
    <property name="ActiveService" type="o" access="read"/>
    <signal name="ServicesChanged">
      <arg type="ao" name="Services" direction="out"/>
    </signal>  
    <signal name="ActiveServiceChanged">
      <arg type="o" name="ActiveService" direction="out"/>
    </signal>  
  </interface>
  <interface name="org.cutie_shell.SettingsDaemon.NetworkService">
    <property name="DisplayName" type="s" access="read"/>
    <property name="Strength" type="y" access="read"/>
    <property name="Wireless" type="b" access="read"/>
    <signal name="StrengthChanged">
      <arg type="y" name="Strength" direction="out"/>
    </signal>  
  </interface>
</node>
